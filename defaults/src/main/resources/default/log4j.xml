<Configuration>
    <Appenders>
        <Null name="disabled"/>
    </Appenders>
    <Loggers>
        <Logger name="org.apache.http.wire" additivity="false" level="error"/>
        <Logger name="org.apache.http.headers" additivity="false" level="error"/>
        <Root level="error">
            <AppenderRef ref="disabled"/>
        </Root>
    </Loggers>
</Configuration>
